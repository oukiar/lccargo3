<div id="ware_table">    <table class="table table-bordered" id="blueRow">    <tr>            <th>Rcpt. #</th>            <th>Client</th>            <th>Shipper</th>            <th>Invoice</th>            <th>Date</th>            <th>Country</th>            <th>Transport</th>            <th>WareHouse</th>    </tr>    <% results.forEach(function(label){%>	<%if(label["Repacked"]==="NO"){%>		 <tr style="">			 <%}else{%>				  <tr style="background:#FFFAF3">					  <%}%>            <td align="center"><%= label["Receipt"]%></td>            <td><%= label["Consignee"]%></td>            <td><%= label["Shipper"]%></td>            <td><%= label["Invoice"]%></td>            <td><%= label["ReceiptDate"]%></td>            <td><%= label["Country"]%></td>            <td><%= label["Transport"]%></td>            <td><%= label["Warehouse"]%></td>			 </tr>		    <% }) %>       </table></div>