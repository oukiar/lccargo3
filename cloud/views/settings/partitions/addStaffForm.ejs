input type="hidden" name="link" value="<%= company.get('companyLink') %>">			             <input type="hidden" name="geocodedata" id="geocodedata">			             <input type="submit"  style="length:50px;" value="submit" class="pure-button pure-button-primary">                         </div>                    </div>                </div>            </div>        </section>			    <script src="https://maps.googleapis.com/maps/api/js?sensor=false&amp;libraries=places"></script>	    <script src="/cod/js/jquery-1.11.2.min.js"></script>  	    <script src="cod/js/jquery.geocomplete.js"></script>	    <script>	      $(function(){        	        var options = {	          map: ".map_canvas",	          location: ""	        };        	        $("#staffAddress").geocomplete(options).bind("geocode:result", function(event, result){				/*alert(JSON.stringify(result) );*/				$("#geocodedata").val(JSON.stringify(result) );			});	      });	    </script>