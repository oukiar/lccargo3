				  <div class="col-md-6 col-xs-6" style="margin-top:8px; margin-top:8px; width:100%; min-width: 850px;">    <div class="panel">			<form class="pure-form" method="post" action="/active_clients_filter" align="center" style="margin-top:21px;">			  			        <input type="text" placeholder="Search" id="searchtext" name="searchtext">			               <label for="filterby" class="pfont">By:</label>			               <select id="filterby" name="filterby" >			                   <option value="Name">Name</option>			                   <option value="Company">Email</option>			                   <option value="Country">Username</option>			                   <option value="Destination">Type</option>			               </select>			   <button type="submit" class="pure-button pure-button-primary">Search</button>						</form>	          	        <div class="panel-body" id="staffTableList">             <ul id="list">	                <table class="table table-bordered" id="blueRow">	                <tr>			          <th>Name</th>			          <th>Job Position</th>			  		  <th>Division</th>					  <th>Telephone</th>					  <th>Cellphone</th>			  	      <th>Email</th>					  <th>Address</th>					  <th>Username</th>					  <th>Type</th>					  <th>Actions</th>	                </tr>                					<% staffs.forEach(function(staFF){%>	                    <tr>                          <td><%= staFF.get("StaffId").get("Name") %></td>						  <td><%= staFF.get("StaffId").get("Position")%></td>						  <td><%= staFF.get("StaffId").get("Division") %></td>						  <td><%= staFF.get("StaffId").get("Telephone")%></td>						  <td><%= staFF.get("StaffId").get("Cellphone")%></td>						  <td><%= staFF.get("email") %></td>						  <td><%= staFF.get("StaffId").get("Address")%></td>						  <td><%= staFF.get("username").split("@")[0]%></td>						  <td><%= staFF.get("Type")%></td> 						  <td align="center" style="white-space:nowrap;"><a href="#editStaffModal" class="btn btn-border btn-orange" onclick="fillStaff('<%= JSON.stringify(staFF)%>', '<%= JSON.stringify(staFF.get('StaffId'))%>', '<%= JSON.stringify(staFF.get('Company'))%>')"><i class="fa fa-pencil-square-o"></i> Edit</a> &nbsp &nbsp <a class="btn btn-border btn-danger" id="<%=staFF.id%>" onclick="byeByeStaff(this.id, '<%=staFF.get('Company').id%>', '/staff_staffList')"><i class="fa fa-trash-o"></i></a></td>     	                    </tr>										    <% }) %>																                </table>		               </ul>		           </div>		       </div>		   </div><script>function fillStaff(objectjson, jsonStaff, company){staff = JSON.parse(objectjson);staffInfo = JSON.parse(jsonStaff);staffId = staff["objectId"];company = JSON.parse(company);companyLink = company["companyLink"];$("#editStaffName").val(staffInfo["Name"]);$("#editStaffJobPosition").val(staffInfo["Position"]);$("#editStaffDivision").val(staffInfo["Division"]);$("#editStaffTelephone").val(staffInfo["Telephone"]);$("#editStaffCellphone").val(staffInfo["Cellphone"]);$("#editStaffEmail").val(staff["email"]);$("#editStaffAddress").val(staffInfo["Address"]);$("#editStaffCompanyLink").val(companyLink);$("#editStaffUsername").val(staff["username"].split("@")[0]);}function editName(){$('#editStaffName').prop('disabled', false);$('#editName').hide();$('#cancelName').show();};</script>