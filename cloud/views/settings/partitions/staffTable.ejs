				  <div class="col-md-6 col-xs-6" style="margin-top:8px; margin-top:8px; width:100%; min-width: 850px;">    <div class="panel">			<form class="pure-form" method="post" action="/active_clients_filter" align="center" style="margin-top:21px;">			  			        <input type="text" placeholder="Search" id="searchtext" name="searchtext">			               <label for="filterby" class="pfont">By:</label>			               <select id="filterby" name="filterby" >			                   <option value="Name">Name</option>			                   <option value="Company">Email</option>			                   <option value="Country">Username</option>			                   <option value="Destination">Type</option>			               </select>			   <button type="submit" class="pure-button pure-button-primary">Search</button>						</form>	          	        <div class="panel-body">             <ul id="list">	                <table class="table table-bordered" id="blueRow">	                <tr>			          <th>Name</th>			          <th>Job Position</th>			  		  <th>Division</th>					  <th>Telephone</th>					  <th>Cellphone</th>			  	      <th>Email</th>					  <th>Address</th>					  <th>Username</th>					  <th>Type</th>	                </tr>                					<% staffs.forEach(function(staff){%>	                    <tr>                            <td><%= staff.get("Name") %></td>							<td><%= staff.get("Position")%></td>							<td><%= staff.get("Division") %></td>						    <td><%= staff.get("Telephone")%></td>							<td><%= staff.get("Cellphone")%></td>							<td><%= staff.get("User").get("email") %></td>							<td><%= staff.get("Address")%></td>							<td><%= staff.get("User").get("username") %></td>							<td><%= staff.get("User").get("Type")%></td>      	                    </tr>				    <% }) %>		                </table>		               </ul>		           </div>		       </div>		   </div>	       	  	  	  