<!DOCTYPE html><html>    <head>		        <meta http-equiv="content-type" content="text/html;charset=UTF-8" />        <meta charset="utf-8" />        <title>LC Cargo</title>        <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" />        <meta content="" name="description" />        <meta content="" name="author" />               	   <!--ADMIN REQUIREMENTS-->     <% include ../generals/ultraAdmin.ejs %>	            <script src="/cod/js/common.js"></script>        <link rel="stylesheet" href="https://ajax.googleapis.com/ajax/libs/jqueryui/1.11.3/themes/smoothness/jquery-ui.css" />        <script src="https://ajax.googleapis.com/ajax/libs/jqueryui/1.11.3/jquery-ui.min.js"></script>	<link rel="stylesheet" href="//code.jquery.com/ui/1.11.4/themes/smoothness/jquery-ui.css">	  <script src="//code.jquery.com/jquery-1.10.2.js"></script>	  <script src="//code.jquery.com/ui/1.11.4/jquery-ui.js"></script>	  <link rel="stylesheet" href="/resources/demos/style.css">	  <script>	  $(function() {	    $( "#tabs" ).tabs();	  });	  </script>        </head>    <!-- END HEAD -->    <!-- BEGIN BODY -->    <body>		<!--START TOPBAR-->		 <% include ../generals/topbar.ejs %>		         <!-- START CONTAINER -->        <div class="page-container row-fluid">        <!-- SIDEBAR - START -->		 <% include partitions/sidebarSettings.ejs %>        <!--  SIDEBAR - END -->						            <!-- START CONTENT -->                     <section class="wrapper" style='margin-top:60px;display:inline-block;width:100%;padding:15px 0 0 15px;'>              <div class='col-lg-12 col-md-12 col-sm-12 col-xs-12'>              <div class="page-title">              <div class="pull-left">		  		              <h1 class="title">Client's List</h1>			  	  			 		 	          <div class="panel">    <div class="panel-body">		<style>		#tabs{			border:none;		}				#tabs ul{border:0;			background: white;				}								#tabs li{					color:#FF7260;color:rgba(0,0,0,.8);border:2px solid #82939E;border:1 rgba(0,0,0,0);background-color:#eee;									}			</style>	  <div id="tabs">	    <ul>	      <li><a href="#tabs-1"><font color="#82939E">List / Edit</font></a></li>	      <li><a href="#tabs-2"><font color="#82939E">Add</font></a></li>	    </ul>			    <div id="tabs-1">					  		  <div class="pull-right"></div>   	             <div class="col-lg-12 col-md-12 container">  			<form class="pure-form" method="post" action="/active_clients_filter" align="right">	  			        <input type="text" placeholder="Search" id="searchtext" name="searchtext">  			               <label for="filterby" class="pfont">By:</label>  			               <select id="filterby" name="filterby" >  			                   <option value="Name">Name</option>  			                   <option value="Company">Company</option>  			                   <option value="Country">Country</option>  			                   <option value="Destination">Destination</option>  			                   <option value="Address">Address</option>  			                   <option value="ID1">ID</option>  			                   <option value="Telephone">Telephone</option>  			                   <option value="Email">Email</option>  			               </select>						    			        <button type="submit" class="pure-button pure-button-primary">Search</button>			  			</form>  	          </div>  	               	                <table class="table table-bordered" id="blueRow">  	                <tr>  			              <th>Name</th>  			              <th>Company</th>  			              <th>Country</th>  			  			<th>Address</th>  			  			<th>Tel</th>  			  			<th>Email</th>  			  			<th>Destination</th>						<th>Status</th>  	                </tr>                  					<% clients.forEach(function(client){%>  	                            <tr dir-paginate="product in product | filter:q |" current-page="currentPage">                              <td><%= client.get("Name")%></td>                              <td><%= client.get("Company")%></td>                              <td><%= client.get("Country")%></td>                              <td><%= client.get("Address")%></td>							                              <td><%= client.get("Telephone")%></td>                              <td><%= client.get("Email")%></td>                              <td><%= client.get("Destination")%></td>							  <td><%= client.get("Status")%></td>                          	                            </tr>  								 <% }) %>  	                </table>  	      </div>	 	    </div><!--end tab-1-->			    <div id="tabs-2">			   		  <% include ../generals/client_register.ejs %>	    </div>					 	  </div>				    	            </div>  	          </div>			  			  			        </div>			        			      </div>			               </div>              </div>            </section>            <!-- END CONTENT -->                         </div><!-- END CONTAINER -->        <!-- LOAD FILES AT PAGE END FOR FASTER LOADING --><%include ../generals/endFiles.ejs%>        <!-- modal end -->            <script>  	$( "#txt_name" ).autocomplete({source: []});   	$("#txt_name").keyup(		function(){			var myval = $("#txt_name").val();						if(myval.length >= 1){				$.post("/autofill_client", 				{"txt_search":myval}, 				function(data){					result = JSON.parse(data);                    newresult = [];                    for(var i=0; i<result.length; i++){                        newresult.push({label:result[i]["Name"], idx:result[i]["objectId"]});                    }										$( "#txt_name" ).autocomplete({source: newresult});   					$('#txt_name').trigger("keydown");				});			}		});                /*CLIENT SELECCION FROM THE RESULT*/        $( "#txt_name" ).autocomplete({             select: 			 function( event, ui ){                    InsertTable({                            cols:   /*Columnas definidas FIELD:TITLE or FIELD:{FIELDNEEDED:FIELDTITLE}*/                                 {                                    "@<input type='checkbox' name='checkware' id='%'>":"Check", /* DINAMIC REPLACE FOR TABLE */                                    "Receipt":"Rcpt. #",                                    "Shipper":{"Name":"Agency"}, /*This is the sintax for pointers showed on table*/                                    "ReceiptDate":"Date",                                    "Country":"Country",                                    "Volume":"Volume",                                    "Warehouse":"WareHouse",                                    "Invoice":"Invoice",                                    "Transport":"Transport"                                },                                                        class:"table table-bordered", /* CSS STYLE */                            collection: "WareHouse", /* COLLECCION PRINCIPAL */                            by: "Consignee",    /* CAMPO DE BUSQUEDA */                            value: {__type: "Pointer", className: "Clients", objectId: ui.item.idx},                            itemid: "tablecontent"                        });                }          });            $("#btn_repack").click(function(){                var consolidate = ''; /* = "Consolidate detail:\n\n"; */                var checkboxes = document.getElementsByName("checkware");              var checkbox;              for(i in checkboxes)        {                        if(checkboxes[i].checked)            {                /*                alert(checkboxes[i].id);                AQUI HAY QUE CREAR EL STRING QUE SE PASARA AL FORM DE WAREHOUSE CON LA INFORMACION PERTINENTE                */                                consolidate += checkboxes[i].id + ',';            }        }        /*        window.open("/staff_warehouse_consolidate/" + consolidate, "Microsoft","height=707,width=878");        */        alert(consolidate);	        return false;    });                           </script>            </body>	</html>