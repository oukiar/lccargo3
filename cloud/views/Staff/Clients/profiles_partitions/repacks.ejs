<div id="tabrepacks"><div class="pull-right"></div><div class="row">    <div class="col-lg-12 col-md-12 container">        <div class="row">            <div class="col-md-6 col-xs-6" style="margin-top:13px;">				            </div>                        <div class="col-md-6 col-xs-6" style="margin-top:10px;">                <form class="pure-form" align="right">   <!-- FIXME, RENEW THIS ALGORITHM -->                    <label for="filterby_repack" class="pfont">By:</label>                                   <select  id="filterby_repack" name="filterby_repack">                        <option value="All">All</option>                        <option value="Agent">Agent</option>                        <option value="Date">Date</option>                        <option value="Shipment">Shipment #</option>                        <option value="Transport">Transport</option>                        <option value="Warehouse">WareHouse</option>                        <option value="Country">Country</option>                    </select>                    <!-- IF ALL OR DATE SELECTED THIS SEARCH INPUT IS NOT USE-->                    <input type="hidden" placeholder="Search"  id="searchtext_repack" name="searchtext_repack">                           <label class="pfont">Date:</label>                    <select id="filterdate_repack" name="filterdate_repack">                        <option value="All">All</option>                        <option value="Today">Today</option>                        <option value="Yesterday">Yesterday</option>                        <option value="This week">This week</option>                        <option value="This month">This month</option>                        <option value="Last month">Last month</option>	                        <option value="Current year">Current Year</option>                    </select>			                                       </form>            </div>        </div>              <div class="panel">            <div class="panel-body">                <%include repacks_dynamictable.ejs%>            </div>        </div>   <script>            function update_table_repack()        {               InsertTable({                    cols:   /*Columnas definidas FIELD:TITLE or FIELD:{FIELDNEEDED:FIELDTITLE}*/                         {                                                        "Agent":"",                            "ReceiptDate":"", /*This is the sintax for return fields of pointers showed on table*/                            "Shipment":"",                            "Transport":"",                            "Warehouse":"",                            "Country":""                        },                                            /* AL PASAR EL PARAMETRO COLLECTION INDICAMOS QUE NECESITAMOS BUSQUEDA CON LISTA DE RESULTADOS */                    collection: "Repacks", /* COLLECCION PRINCIPAL */                                        /* CONDICIONES */                    conditions: filter_conditions("filterby_repack", "searchtext_repack", "filterdate_repack"),                    by: "Client",    /* CAMPO DE BUSQUEDA */                    value: {__type: "Pointer", className: "Clients", objectId: usrID},                                        /*BUSQUEDA DE SEARCH TIPO LIKE*/                    like: filter_like("filterby_repack", "searchtext_repack"),                                        document: "Staff/Clients/profiles_partitions/repacks_dynamictable",                    itemid: "repackstable"                });                                }        function searchtext_keyup_repack()        {            update_table_repack();        }                            $("#filterdate_repack").change(function(){            update_table_repack();        });          $("#filterby_repack").change(function()            {                filterby_changed("filterby_repack", "searchtext_repack", "filterdate_repack", update_table_repack, searchtext_keyup_repack);             });	  	</script>						    </div>			</div>     			</div>			