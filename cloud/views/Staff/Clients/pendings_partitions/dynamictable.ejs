<div id="clientstable">      <ul id="list">        <table class="table table-bordered" id="blueRow">        <tr>            <th>Activate</th>            <th>Name</th>            <th>Company</th>            <th>Country</th>            <th>Tel</th>            <th>Email</th>			<th>Account ID</th>			<th>Actions</th>        </tr>        <% results.forEach(function(client){%>        <tr dir-paginate="product in product | filter:q |" current-page="currentPage">        <td><a href="#openModal" onclick="fillClient('<%= JSON.stringify(client)%>')">Activate</a></td>        <td><%= client["Name"]%></td>        <td><% if(typeof(client["Company"])!="undefined"){%><%= client["Company"]%><%}else{%>-<%}%></td>        <td><% if(typeof(client["Country"])!="undefined"){%><%= client["Country"]%><%}else{%>-<%}%></td>        <td><%= client["Telephone"]%></td>        <td><a href="mailto:<%= client["Email"]%>"><%= client["Email"]%></a></td>		<td><%= client["AccountID"]%></td>	    <td align="center"><a href="/#" class="btn btn-border btn-orange"><i class="fa fa-pencil-square-o"></i> Edit</a> &nbsp &nbsp <a class="btn btn-border btn-danger" id="<%=client["objectId"] %>" onclick="byeByeClient(this.id,'<%= companyID %>', '/inactive_clients')"><i class="fa fa-trash-o"></i></a></td>         </tr>         <% }) %>        </table>      </ul></div><script>    var clientID;    function fillClient(objectjson)    {        client = JSON.parse(objectjson);                clientID = client["objectId"];                $("#clientName").html(client["Name"]);        $("#clientTel").html(client["Telephone"]);        $("#clientEmail").html(client["Email"]);           }</script>