<div id="clientstable">      <ul id="list">        <table class="table table-bordered" id="blueRow">        <tr>            <th>Activate</th>            <th>Name</th>            <th>Company</th>            <th>Country</th>            <th>Address</th>            <th>Tel</th>            <th>Email</th>            <th>Destination</th>			<th>Delete</th>        </tr>        <% results.forEach(function(client){%>        <tr dir-paginate="product in product | filter:q |" current-page="currentPage">        <td><a href="#openModal" onclick="fillClient('<%= JSON.stringify(client)%>')">Activate</a></td>        <td><%= client["Name"]%></td>        <td><%= client["Company"]%></td>        <td><%= client["Country"]%></td>        <td><%= client["Address"]%></td>        <td><%= client["Telephone"]%></td>        <td><a href="mailto:<%= client["Email"]%>"><%= client["Email"]%></a></td>        <td><%= client["Destination"]%></td>	    <td align="center"><a class="btn btn-border btn-danger" id="<%=client["objectId"] %>" onclick="byeBye(this.id,'<%=Parse.User.current().id%>', '/savedArticles')"><i class="fa fa-trash-o"></i></a></td>         </tr>         <% }) %>        </table>      </ul></div><script>function fillClient(objectjson){    client = JSON.parse(objectjson);        $("#clientName").html(client["Name"]);    $("#clientTel").html(client["Telephone"]);    $("#clientEmail").html(client["Email"]);    $("#clientID").val(client["objectId"]);   }</script>