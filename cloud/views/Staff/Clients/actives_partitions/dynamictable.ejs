<div id="clientstable">      <ul id="list">        <table class="table table-bordered">        <tr>            <th>Name</th>            <th>Company</th>            <th>Country</th>            <th>Address</th>            <th>Tel</th>            <th>Email</th>            <th>Destination</th>        </tr>        <% results.forEach(function(client){%>                    <tr dir-paginate="product in product | filter:q |" current-page="currentPage">                <td><%= client["Name"]%></td>                <td><%= client["Company"]%></td>                <td><%= client["Country"]%></td>                <td><%= client["Address"]%></td>                                <td><%= client["Telephone"]%></td>               <td><a href="mailto:<%= client["Email"]%>"><%= client["Email"]%></a></td>                <td><%= client["Destination"]%></td>                                </tr>                     <% }) %>        </table>      </ul></div>