<div id="ware_table">    <table class="table table-bordered" id="blueRow">    <tr>            <th>Rcpt. #</th>            <th>Client</th>            <th>Shipper</th>            <th>Invoice</th>            <th>Date</th>            <th>Country</th>            <th>Transport</th>            <th>WareHouse</th>            <th id="classifycolumn" style="display:none">Ship</th>    </tr>    <% results.forEach(function(label){%>	<%if(label["Status"]==="Received"){%>		 <tr style="">			 <%}else if(label["Status"]==="Repack"){%>				  <tr style="background:#FFFAF3">					  <%}else if(label["Status"]==="NotProcessed"){%>				  <tr style="background:#F3FAF0">					  <%}%>            <td align="center"><%= label["Receipt"]%></td>            <td><%= label["Consignee"]%></td>            <td><% if(typeof label["Shipper"] != "undefined"){%> <%=label["Shipper"]%><%}%></td>            <td><%= label["Invoice"]%></td>            <td><%= label["ReceiptDate"]%></td>            <td><%= label["Country"]%></td>            <td><%= label["Transport"]%></td>            <td><%= label["Warehouse"]%></td>            <td style="display:none" class="checkship"><input name="checkship" id="<%=label['objectId']%>" type="checkbox"></td>			 </tr>		    <% }) %>       </table></div><div id="openModal" class="modalDialog">	<div>		<a href="#close" title="Close" class="close">X</a>		<h2>Saving the shipment</h2>                <select id="shipment" name="shipment">            <option value="Aereal">Aereal</option>            <option value="Terrestral">Terrestral</option>            <option value="Sea">Sea</option>        </select>                <button id="btn_save">Save</button>                <div id="classify_items">        </div></div><script>    $("#btn_classify").click(        function()        {            $("#classifycolumn").css("display", "block");            $("#shipoptions").css("display", "block");                        $(".checkship").css("display", "block");        });            $("#btn_accept").click(        function()        {            checkboxes = document.getElementsByName("checkship");                        for(i in checkboxes)            {                if(checkboxes[i].checked)                {                            return true;                }            }                        alert("You must select almost one label");            return false;        });    $("#btn_save").click(        function()        {            checkboxes = document.getElementsByName("checkship");                        for(i in checkboxes)            {                if(checkboxes[i].checked)                {                                            var str = "<img id='save-" + checkboxes[i].id + "' src='/web/img/icons/loader_grey.gif' width=16 height=16>  " + checkboxes[i].id + "<br>";                                            $("#classify_items").append( str );                                        kwargs = {                                collection:"Warehouses",                                 objectId: checkboxes[i].id,                                values:{Status:"NotProcessed", "Shipment":$("#shipment").val()}                            };                                                                                           $.post("/update",                         kwargs,                         function(data)                        {                            kwargs = JSON.parse(data);                            $("#save-" + kwargs["objectId"]).replaceWith("SAVED");                        }                    );                }            }                        /* ACTUALIZAR TABLA, PARA QUE YA NO SE MUESTREN LOS REGISTROS QUE SE ACABAN DE CLASIFICAR */            update_table();        });</script>