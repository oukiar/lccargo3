<button id="btn_classify">Classify</button><div id="shipoptions" style="display:none">        <select>        <option>Aereal</option>        <option>Terrestral</option>        <option>Sea</option>    </select>        <button id="btn_accept">Accept</button></div><div id="ware_table">    <table class="table table-bordered" id="blueRow">    <tr>            <th>Rcpt. #</th>            <th>Client</th>            <th>Shipper</th>            <th>Invoice</th>            <th>Date</th>            <th>Country</th>            <th>Transport</th>            <th>WareHouse</th>            <th id="classifycolumn" style="display:none">Ship</th>    </tr>    <% results.forEach(function(label){%>	<%if(label["Repacked"]==="NO"){%>		 <tr style="">			 <%}else{%>				  <tr style="background:#FFFAF3">					  <%}%>            <td align="center"><%= label["Receipt"]%></td>            <td><%= label["Consignee"]%></td>            <td><% if(typeof label["Shipper"] != "undefined"){%> <%=label["Shipper"]%><%}%></td>            <td><%= label["Invoice"]%></td>            <td><%= label["ReceiptDate"]%></td>            <td><%= label["Country"]%></td>            <td><%= label["Transport"]%></td>            <td><%= label["Warehouse"]%></td>            <td style="display:none" class="checkship"><input name="checkship" id="<%=label['objectId']%>" type="checkbox"></td>			 </tr>		    <% }) %>       </table></div><div id="openModal" class="modalDialog">	<div>		<a href="#close" title="Close" class="close">X</a>		<h2>Select the shipment transport type</h2>            <button>Aereal</button><br>            <button>Terrestral</button><br>            <button>Sea</button>        </div></div><script>    $("#btn_classify").click(        function()        {            $("#classifycolumn").css("display", "block");            $("#shipoptions").css("display", "block");                        $(".checkship").css("display", "block");        });    $("#btn_accept").click(        function()        {            var ships = [];                        checkboxes = document.getElementsByName("checkship");                        for(i in checkboxes)            {                if(checkboxes[i].checked)                {                    ships.push(checkboxes[i].id);                }            }                        alert(JSON.stringify( ships ));        });</script>