		<script type="text/javascript" src="/receipt/js/jquery.js"></script>
		<script type="text/javascript" src="/receipt/js/jqueryui.js"></script>
		<script type="text/javascript" src="js/menu.js"></script>
		<script type="text/javascript" src="js/menu-horizontal-dropdown.js"></script>
		<script type="text/javascript" src="js/menu-horizontal-dropdown-1.js"></script>
		<script type="text/javascript" src="js/index.js"></script>
        
        <script type="text/javascript" src="js/index.js"></script>
        
        <script type="text/javascript" src="/cod/js/qrcode/qrcode.js"></script>
        
		<script type="text/javascript">
			var androidBrowserVersion = RegExp(/Mozilla\/5\.0 \(Linux; .; Android ([\d.]+)/).exec(navigator.userAgent);
            if(androidBrowserVersion && parseFloat(androidBrowserVersion[1]) < 5) 
            { 
                document.getElementsByTagName('body')[0].className+=' whitespacefix'; 
            }
            
            /*  */
            new QRCode(document.getElementById("qrcode_receipt"), "<%= JSON.stringify({objectId:receipt.id, type:"Receipt"})%>");

            
		</script>
