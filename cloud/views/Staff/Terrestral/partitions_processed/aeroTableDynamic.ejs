<div id="aeroTableDynamic">  <ul id="list">    <table class="table table-bordered" id="blueRow">    <tr>          <th>Receipt</th>		   <th>Client</th>          <th>Repack Price</th>          <th>Volume</th>        <th>Content</th>		<th>Agent</th>	<th>Processed</th>       </tr> <% results.forEach(function(label){                %>             <tr dir-paginate="product in product | filter:q |" current-page="currentPage">         <td><a onclick="wareceipt('<%=label['objectId']%>')" href="#openModal"><%= label["ReceiptNum"]%></a></td>         <td><%= label["Consignee"]%></td>         <td><%= label["Value"]%></td>         <td><%= label["Volume"]%></td>         <td><%= label["Content"]%></td>		<td><%= label["Agent"]%></td>             <td align="center">            <a id="<%=label['objectId']%>" class="btn btn-orange btn-md myclass"> &nbsp; Sent </a>          </td>             </tr>              <% }) %>    </table>  </ul></div><script>$(".myclass").click(function(){                var Receipts = Parse.Object.extend("Receipts");        var receipt = new Receipts();                receipt.id = this.id;        receipt.set("Status", "Sent");                receipt.save({          success: function(client) {            update_table();          },          error: function(client, error) {          }        });            });</script>