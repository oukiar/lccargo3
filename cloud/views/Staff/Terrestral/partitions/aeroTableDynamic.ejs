<div id="aeroTableDynamic">  <ul id="list">    <table class="table table-bordered" id="blueRow">    <tr>          <th>Receipt</th>		   <th>Client</th>          <th>Weight</th>          <th>Volume</th>        <th>Content</th>        <th>Boxes</th>		<th>Agent</th>       </tr> <% results.forEach(function(label){        var boxes = 0;        if(label["Boxes"] != "")            boxes = JSON.parse(label["Boxes"]).length;        %>             <tr dir-paginate="product in product | filter:q |" current-page="currentPage">         <td><a onclick="wareceipt('<%=label['objectId']%>')" href="#openModal"><%= label["Receipt"]%></a></td>         <td><%= label["Consignee"]%></td>         <td><%= label["Weight"]%></td>         <td><%= label["Volume"]%></td>         <td><%= label["Content"]%></td>         <td><%= boxes %></td>		<td><%= label["Agent"]%></td>                  </tr>              <% }) %>    </table>  </ul></div>