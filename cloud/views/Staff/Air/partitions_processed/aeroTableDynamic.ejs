<div id="aeroTableDynamic">  <ul id="list"> <table class="table table-bordered" id="blueRow">	<style>	#not:nth-child(even) {background: white}	#not:nth-child(odd) {background: #FBFBFC}	</style> <tr>       <th>Receipt</th>	   <th>Client</th>       <th>Repack Price</th>       <th>Boxes</th>     <th>Content</th>	<th>Agent</th>	<th>Processed</th> </tr> <% results.forEach(function(label){        %>		       <tr id="not">       <td><a onclick="wareceipt('<%=label['objectId']%>')" href="#openModal"><%= label["ReceiptNum"]%></a></td>       <td><%= label["Consignee"]%></td>       <td><%= label["Value"]%></td>       <td><%= label["TotalBoxes"]%></td>       <td><%= label["Content"]%></td>	   <td><%= label["Agent"]%></td>        <td align="center">            <a id="<%=label['objectId']%>" class="btn btn-orange btn-md myclass"> &nbsp; Sent </a>          </td>             </tr>              <% }) %> </table>  </ul></div><script>$(".myclass").click(function(){                var Receipts = Parse.Object.extend("Receipts");        var receipt = new Receipts();                receipt.id = this.id;        receipt.set("Status", "Sent");                receipt.save({          success: function(client) {            update_table();          },          error: function(client, error) {          }        });            });</script>