<div id="pendingagencies_dynamictable">    <ul id="list">    <table class="table table-bordered">    <tr>        <th>Activate</th>        <th>Name</th>        <th>Website</th>        <th>Address</th>        <th>Country</th>        <th>Telephone</th>        <th>Email</th>    </tr>        <% results.forEach(function(agency) { %>                <tr dir-paginate="product in product | filter:q |" current-page="currentPage">            <td><a href="#openModal" onclick="fillStore('<%= JSON.stringify(agency)%>')">Activate</a></td>            <td><%= agency["Name"]%></td>            <td><a href="http://<%= agency["Website"]%>" target="blank"><%= agency["Website"]%></a></td>            <td><%= agency["Address"]%></td>            <td><%= agency["Country"]%></td>            <td><%= agency["Telephone"]%></td>            <td><a href="mailto:<%= agency["Email"]%>"><%= agency["Email"]%></a></td>                        </tr>                 <% }) %>    </table>    </ul></div>	     <script>    var agencyID;function fillStore(objectjson){    agency = JSON.parse(objectjson);        agencyID = agency["objectId"];        $("#storeName").html(agency["Name"]);    $("#storeEmail").html(agency["Email"]);    $("#storeTel").html(agency["Telephone"]);    $("#storeWeb").html(agency["Website"]);    $("#storeAddr").html(agency["Address"]);        $("#agencyID").val(agency["objectId"]);        }</script>	  	  	  	  