<div id="dynamictable">    <ul id="list">    <table class="table table-bordered" id="blueRow">    <tr>        <th>Activate</th>        <th>Name</th>        <th>Website</th>        <th>Address</th>        <th>Country</th>        <th>Telephone</th>        <th>Email</th>    </tr>        <% results.forEach(function(agency) { %>                <tr dir-paginate="product in product | filter:q |" current-page="currentPage">            <td><a href="#openModal" onclick="fillStore('<%= JSON.stringify(agency)%>')">Activate</a></td>            <td><%= agency["Name"]%></td>                        <%if(typeof(agency["Website"]) == "undefined"){%>                <td>-</a></td>            <%}else{%>            <td><a href="http://<%= agency["Website"]%>" target="blank"><%= agency["Website"]%></a></td>            <%}%>                        <td><%= agency["Address"]%></td>            <td><%= agency["Country"]%></td>            <td><%= agency["Telephone"]%></td>            <td><a href="mailto:<%= agency["Email"]%>"><%= agency["Email"]%></a></td>                        </tr>                 <% }) %>    </table>    </ul></div>	     <script>function fillStore(objectjson){    agency = JSON.parse(objectjson);        $("#storeName").html(agency["Name"]);    $("#storeEmail").html(agency["Email"]);    $("#storeTel").html(agency["Telephone"]);    $("#storeWeb").html(agency["Website"]);    $("#storeAddr").html(agency["Address"]);        $("#agencyID").val(agency["objectId"]);        }</script>	  	  	  	  