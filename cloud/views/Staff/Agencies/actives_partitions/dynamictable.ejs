<div id="activeagencies_dynamictable">    <ul id="list">        <table class="table table-bordered" id="blueRow">            <tr>                <th>Name</th>                <th>Website</th>                <th>Address</th>                <th>Country</th>                <th>State</th>                <th>Telephone</th>                <th>Email</th>            </tr>            <% results.forEach(function(agency) { %>            <tr dir-paginate="product in product | filter:q |" current-page="currentPage">                <td><%= agency["Name"]%></td>                <td><a href="http://<%= agency["Website"]%>" target="blank"><%= agency["Website"]%></a></td>                <td><%= agency["Address"]%></td>                <td><%= agency["Country"]%></td>                <td><%= agency["State"]%></td>                <td><%= agency["Telephone"]%></td>                <td><a href="mailto:<%= agency["Email"]%>"><%= agency["Email"]%></a></td>            </tr>            <% }) %>        </table>    </ul></div>	     	  	  	  	  