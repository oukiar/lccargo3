lass="row">	  	      <div class="col-lg-12 col-md-12 container">	  	          <div class="row">               <div class="col-md-6 col-xs-6" style="margin-top:10px;">				   <div style="float:left;">					   </div>			   </div>	  	            <div class="col-md-6 col-xs-6" style="margin-top:10px;">					<div style="float:right">			Search agency: <input id="txt_name" name="txt_name">  </div>    </div>	                   </div> 	          <div class="panel">    <div class="panel-body">		<style>		#tabs{			border:none;		}				#tabs ul{border:0;			background: white;				}								#tabs li{					color:#FF7260;color:rgba(0,0,0,.8);border:2px solid #82939E;border:1 rgba(0,0,0,0);background-color:#eee;									}			</style>	  <div id="tabs">	    <ul>	      <li><a href="#tabs-1"><font color="#82939E">Information</font></a></li>	      <li><a href="#tabs-2"><font color="#82939E">Warehouse</font></a></li>	      <li><a href="#tabs-3"><font color="#82939E">Map</font></a></li>		  	    </ul>	    <div id="tabs-1">            <div id="profileinfo">                Please first search and select an agency            </div>	    </div>        	    <div id="tabs-2">            <div id="agency_warehouses">                Please first search and select an agency            </div>	    </div>					    <div id="tabs-3">		HERE THERE WILL BE A MAP!	    </div>	  </div>				    	            </div>  	          </div>			  			  			        </div>			        			      </div>			  </fieldset>              </div>              </div>            </section>            <!-- END CONTENT -->                         </div><!-- END CONTAINER -->        <!-- LOAD FILES AT PAGE END FOR FASTER LOADING -->  <%include ../../generals/endFiles.ejs%>        <!-- modal end -->            <script>  	$( "#txt_name" ).autocomplete({source: []});   	$("#txt_name").keyup(		function(){			if($("#txt_name").val().length >= 1){				$.post("/searchby",                     {                        cols:{objectId:"", Name:""},                        collection:"Agencies",                        like:{Name:$("#txt_name").val()}                    },                     function(data){                        result = JSON.parse(data);                        newresult = [];                        for(var i=0; i<result.length; i++){                            newresult.push({label:result[i]["Name"], idx:result[i]["objectId"]});                        }                                                $( "#txt_name" ).autocomplete({source: newresult});                           $('#txt_name').trigger("keydown");				});			}		});                /*CLIENT SELECCION FROM THE RESULT*/        $( "#txt_name" ).autocomplete({             select: 			 function( event, ui ){                             agencyID = ui.item.idx;                                                                /* PROFILE */                GetRendered({                    collection: "Agencies",                    by: "objectId",                    value: agencyID,                    document: "Staff/Agencies/profiles_partitions/information",                    itemid: "profileinfo"                    });                                                    InsertTable({                            cols:                                 {                                    "Receipt":"",                                    "Agent":"",                                    "Consignee":{"Name":""},                                    "Invoice":"",                                    "Country":"",                                    "Transport":"",                                    "Volume":"",                                    "Warehouse":""                                },                                                        collection: "Warehouses",                            by: "Shipper",                            value: {__type: "Pointer", className: "Agencies", objectId: agencyID},                            document: "Staff/Agencies/profiles_partitions/warehouses",                            itemid: "agency_warehouses"                        });                                             }          });            $("#btn_repack").click(function(){                var consolidate = ''; /* = "Consolidate detail:\n\n"; */                var checkboxes = document.getElementsByName("checkware");              var checkbox;              for(i in checkboxes)        {                        if(checkboxes[i].checked)            {                /*                alert(checkboxes[i].id);                AQUI HAY QUE CREAR EL STRING QUE SE PASARA AL FORM DE WAREHOUSE CON LA INFORMACION PERTINENTE                */                                consolidate += checkboxes[i].id + ',';            }        }        /*        window.open("/staff_warehouse_consolidate/" + consolidate, "Microsoft","height=707,width=878");        */        alert(consolidate);	        return false;    });                           </script>            </body>	</html>