<div id="aeroTableDynamic">  <ul id="list">   <table class="table table-bordered" id="blueRow">   <tr>         <th>Receipt</th>	   <th>Client</th>         <th>Weight</th>         <th>Volume</th>       <th>Content</th>       <th>Boxes</th>	<th>Agent</th>     </tr>   <% results.forEach(function(repack){%>               <tr dir-paginate="product in product | filter:q |" current-page="currentPage">           <td><%= repack["Receipt"]%></td>           <td>CLIENT NAME HERE</td>           <td><%= repack["Weight"]%></td>           <td><%= repack["Volume"]%></td>           <td><%= repack["Content"]%></td>           <td># OF BOXES</td>		<td><%= repack["Agent"]%></td>                      </tr>                <% }) %>   </table>  </ul></div>