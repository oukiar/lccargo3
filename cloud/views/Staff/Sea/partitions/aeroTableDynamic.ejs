<div id="aeroTableDynamic">  <ul id="list">    <table class="table table-bordered" id="blueRow">    <tr>          <th>Receipt</th>          <th>Internal notes</th>          <th>Weight</th>        <th>Volume</th>        <th>Agent</th>        <th>Country</th>        <th>Warehouse</th>    </tr>    <% results.forEach(function(repack){%>                <tr dir-paginate="product in product | filter:q |" current-page="currentPage">            <td><%= repack["Receipt"]%></td>            <td><%= repack["InternalNotes"]%></td>            <td><%= repack["Weight"]%></td>            <td><%= repack["Volume"]%></td>                        <td><%= repack["Agent"]%></td>            <td><%= repack["Country"]%></td>            <td><%= repack["Warehouse"]%></td>                        </tr>                 <% }) %>    </table>  </ul></div>