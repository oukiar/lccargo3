.length >= 1){					$.post("/autofill_client", 							{"txt_search":myval}, 							function(data){								result = JSON.parse(data);		                        newresult = [];								for(var i=0; i<result.length; i++){								newresult.push(result[i]["Name"]);								 }		                        $( "#txt_consignee" ).autocomplete({source: newresult});		                        $('#txt_consignee').trigger("keydown");		                        consigneeresults = result;							});							}			});		    $( "#txt_consignee" ).autocomplete({source: []});   		    $( "#txt_consignee" ).autocomplete(		         {select: 					 function( event, ui ){         		                    for(var i=0; i<consigneeresults.length; i++){		                       if(consigneeresults[i]["Name"] == ui.item.value){		                                                    $("#usrCountry").val(consigneeresults[i]["Country"]);							     $("#clientID").val(consigneeresults[i]["objectId"]);		                                                }		                                            }		                                        }  		                                    }		                                );                                                     var totalrows = 1;                                                    $( "#btn_addbox" ).click(                function()                {                                    totalrows += 1;      var newrow = "<div class=\"cont-106\">";     var beforestr0 = "<div class=\"colwrapper-60\"><div class=\"cont-107\"><input type=\"text\" name=\"";     var beforestr1 = "<div class=\"colwrapper-61\"><div class=\"cont-107\"><input type=\"text\" name=\"";     var beforestr2 = "<div class=\"colwrapper-62\"><div class=\"cont-107\"><input type=\"text\" name=\"";     var beforestr3 = "<div class=\"colwrapper-63\"><div class=\"cont-107\"><input type=\"text\" name=\"";     var beforestr4 = "<div class=\"colwrapper-64\"><div class=\"cont-