<% var newreg = JSON.parse(params); %>

<div id="modalmessage">
	<div style="position:relative; left:18%;">
		<table>
			<tr>
				<th>
                    <h2>New receipt created!</h2>
                </th>
            </tr>

            <tr>
                <td align="center" >
                    <a onclick="window.open('/printlabels/' + '<%= newreg["objectId"]%>','Microsoft')" class="btn btn-orange btn-md"><i class="fa fa-print"></i> &nbsp; Print Labels </a>  
                <td>
            </tr>

            <tr>
                <td align="center">
                    <a href="#close" class="pure-button pure-button-primary">Ok</a>
                </td>
            </tr>

        </table>
    </div>
</div>
