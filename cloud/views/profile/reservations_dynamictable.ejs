<div id="reservationstable">    <ul id="list">        <table class="table table-bordered" id="blueRow">            <tr>                <th>Airline</th>                <th>Arrivals</th>                <th>Departure</th>                <th>Destination</th>                <th>Flight</th>                <th>People</th>                <th>Status</th>                <th>Notes</th>            </tr>            <% results.forEach(function(label){%>                <tr>                    <td align="center"><%= label["Airline"]%></td>                    <td><%= label["Arrival"]%></td>                    <td><%= label["Departure"]%></td>                    <td><%= label["Destination"]%></td>                    <td><%= label["Flight"]%></td>                    <td><%= label["People"]%></td>                    <td><%= label["Status"]%></td>                    <td><%= label["Notes"]%></td>                </tr>            <% }) %>                </table>    </ul></div>